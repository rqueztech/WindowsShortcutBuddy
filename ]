package main

import (
    "database/sql"
    "log"
    "fmt"

    _ "github.com/mattn/go-sqlite3"
)

func main() {
    // Open a connection to the database
    db, err := sql.Open("sqlite3", "./windowsshortcuts.db")
    if err != nil {
        log.Fatal(err)
    } 

    defer db.Close()

    rows, err := db.Query("SELECT * FROM windowsshortcuts;")
    for _, x := range rows {
        fmt.Println(x)
    }
}
